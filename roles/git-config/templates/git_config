[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[alias]
	c = checkout
	s = status -s
	b = branch
	l = log --oneline
[pager]
	branch = false
	log = false
[commit]
	gpgsign = true
[gpg]
	program = gpg2
[user]
	name = {{ git_user_name }}
	email = {{ git_user_email }}
